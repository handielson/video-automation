{
    "buildCommand": "npm run build",
    "outputDirectory": "dist",
    "framework": "vite",
    "installCommand": "npm install",
    "functions": {
        "api/merge-video.ts": {
            "maxDuration": 60,
            "memory": 1024
        }
    },
    "crons": [
        {
            "path": "/api/generate-video",
            "schedule": "0 12 * * *"
        },
        {
            "path": "/api/generate-video",
            "schedule": "0 18 * * *"
        }
    ]
}